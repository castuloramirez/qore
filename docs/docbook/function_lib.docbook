      <listitem>
	<programlisting><command>my</command> string $str = format_date("Day, Mon D, YYYY-MM-DD HH:mm:SS", 2004-02-01T12:30:00);
# returns "Sunday, Feb 1, 2004-02-01 12:30:00"</programlisting>
      </listitem>
    </varlistentry>
