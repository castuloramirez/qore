        <row>
          <entry><para><code><link linkend="rand">rand</link>() <command>returns</command> <link linkend="int_type">int</link></code></para></entry>
          <entry><para>N</para></entry>
          <entry><para>Returns a random integer number.</para></entry>
        </row>

      <row>
        <entry>
