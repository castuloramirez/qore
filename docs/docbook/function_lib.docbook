        </listitem>
      </varlistentry>
      <varlistentry>
        <term>Example</term>
        <listitem>
          <programlisting>$bin = blowfish_encrypt_cbc("hello there", $key);</programlisting>
        </listitem>
      </varlistentry>
    </variablelist>
