#!/usr/bin/env qore
%new-style

%requires ../../../../qlib/QUnit.qm

%require-types
%enable-all-warnings

%requires ../../../../qlib/Util.qm

%exec-class SameTest

public class SameTest inherits QUnit::Test {
    constructor() : Test("SameTest", "1.0") {
        testFunctions = (
            ("func": \testSameBasics(), "name": "Basic tests for the same() function"),
        );
	set_return_value(main());
    }

    testSameBasics() {
        testAssertion(\same(), (list(1, 1),)); # non-uniq number list
        testAssertion(\same(), (list("a", "b", "a"),), RESULT_FAILURE); # non-uniq string list
        testAssertion(\same(), (list(1, 1, 2),), RESULT_FAILURE); # non-uniq number list
        testAssertion(\same(), (list("a", "a"),)); # uniq number list
    }
}
