#!/usr/bin/env qore
%require-types
%enable-all-warnings
%new-style
%requires Util
%requires UnitTest

UnitTest t();

t.ok(slice((1, 2, 3, 4), (2, 3)) == (3, 4), "got some slice");
t.ok(slice((1, 2, 3, 4), (5, 6)) == list(), "slice is empty");
t.ok(slice((1, 2, 3, 4), list()) == list(), "empty slicing list");
t.ok(slice(list(), (2, 3)) == list(), "empty source list");
