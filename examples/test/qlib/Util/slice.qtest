#!/usr/bin/env qore
%new-style

%requires ../../../../qlib/QUnit.qm

%require-types
%enable-all-warnings

%requires ../../../../qlib/Util.qm

%exec-class SliceTest

public class SliceTest inherits QUnit::Test {
    constructor() : Test("SliceTest", "1.0") {
        testFunctions = (
            ("func": \testSliceBasic(), "name": "Basic slice() tests"),
        );
	set_return_value(main());
    }

    testSliceBasic() {
        testAssertion(\slice(), ((1, 2, 3, 4), (2, 3)), new TestResultValue((3, 4)));
        testAssertion(\slice(), ((1, 2, 3, 4), (5, 6)), new TestResultValue(()));
        testAssertion(\slice(), ((1, 2, 3, 4), ()), new TestResultValue(()));
        testAssertion(\slice(), ((), (2, 3)), new TestResultValue(()));
    }
}
