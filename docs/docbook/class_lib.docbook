	<varlistentry>
	  <term>Example</term>
	  <listitem>
	    <programlisting><command>my</command> *string $dir = $pgm.getScriptDir();</programlisting>
	  </listitem>
	</varlistentry>
      </variablelist>

      <table>
	<title>Return Values for Program::getScriptDir()</title>
	<tgroup cols='2' align='left' colsep='1' rowsep='1'>
