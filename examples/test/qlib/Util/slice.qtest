#!/usr/bin/env qore
%require-types
%enable-all-warnings
%new-style
%requires UnitTest
%requires ../../../../qlib/Util.qm

UnitTest t();

t.ok(slice((1, 2, 3, 4), (2, 3)) == (3, 4), "got some slice");
t.cmp(slice((1, 2, 3, 4), (5, 6)), (), "slice is empty");
t.ok(slice((1, 2, 3, 4), list()) == list(), "empty slicing list");
t.cmp(slice(list(), (2, 3)), (), "empty source list");
