#!/usr/bin/env qr
%require-types
%enable-all-warnings

%requires ../../../../qlib/QUnit.qm

%exec-class ExistsFunctionTest

# The function we test for
any sub simple_shift() {
    return shift argv;
}

public class ExistsFunctionTest inherits QUnit::Test {
    constructor() : Test("ExistsFunctionTest", "1.0") {
        addTestCase("Simple existsFunction() test", \existsFunctionTestMethod(), NOTHING);

        # Return for compatibility with test harness that checks return value.
        set_return_value(main());
    }

    existsFunctionTestMethod() {
        testAssertion("existsFunction() call", \existsFunction(), ("simple_shift",));
    }
}

