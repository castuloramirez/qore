	  <term>Prototype</term>
	  <listitem>
              <para><code><link linkend="any_type">any</link> <link linkend="Datasource::selectRows">Datasource::selectRows</link>(<link linkend="string_type">string</link> <replaceable>$sql</replaceable>, ...)</code></para>
              <para><code><link linkend="nothing_type">nothing</link> <link linkend="Datasource::selectRows">Datasource::selectRows</link>() (<link linkend="RT_NOOP_flag">RT_NOOP</link>)</code></para>
	  </listitem>
	</varlistentry>
	<varlistentry>
	  <term>Example</term>
	  <listitem>
	    <programlisting>$list = $db.selectRows("select * from example_table");</programlisting>
	  </listitem>
	</varlistentry>
