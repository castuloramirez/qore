      <term>Example</term>
      <listitem>
	<programlisting><command>my</command> *string $hostname = gethostbyaddr("192.168.0.33");</programlisting>
      </listitem>
    </varlistentry>
    <varlistentry>
      <term>Restrictions</term>
      <listitem>
	<para>Not available with <code><link linkend="no-external-info">PO_NO_EXTERNAL_INFO</link></code></para>
      </listitem>
    </varlistentry>
