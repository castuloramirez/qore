#!/usr/bin/env qore
%require-types
%enable-all-warnings
%requires Qorize
%requires UnitTest

my UnitTest $t();

const OLDSTYLE = False;
const NEWSTYLE = True;

my hash $h1 = (
    "key1" : "some string",
    "key2" : 123,
);

$t.cmploop(\qorizeNamed(), (
    (
        "args" : ($h1, "name", OLDSTYLE),
        "cmp"  : 'my hash $name;
$name."key1" = "some string";
$name."key2" = 123;
',
        "msg"  : "oldstyle",
    ),
    (
        "args" : ($h1, "name", NEWSTYLE),
        "cmp"  : 'hash name;
name."key1" = "some string";
name."key2" = 123;
',
        "msg"  : "newstyle",
    ),
));
