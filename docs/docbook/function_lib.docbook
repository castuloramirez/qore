      <term>Prototype</term>
      <listitem>
	<para><code><link linkend="getAllThreadCallStacks">getAllThreadCallStacks</link>() <command>returns</command> <link linkend="hash_type">hash</link></code></para>
      </listitem>
    </varlistentry>
    <varlistentry>
      <term>Example</term>
      <listitem>
            <para><code><link linkend="getAllThreadCallStacks">getAllThreadCallStacks</link>() <command>returns</command> <link linkend="hash_type">hash</link></code></para>
      </listitem>
    </varlistentry>
    <varlistentry>
      <term>Restrictions</term>
      <listitem>
	<para>Not available with <code><link linkend="no-thread-control">PO_NO_THREAD_CONTROL</link></code> or <code><link linkend="no-thread-info">PO_NO_THREAD_INFO</link></code></para>
      </listitem>
    </varlistentry>

  </variablelist>
