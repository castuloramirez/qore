          <listitem>
              <para><code><link linkend="nothing_type">nothing</link> <link linkend="HTTPClient_setProxyURL">HTTPClient::setProxyURL</link>()</code></para>
              <para><code><link linkend="nothing_type">nothing</link> <link linkend="HTTPClient_setProxyURL">HTTPClient::setProxyURL</link>(<link linkend="string_type">string</link> <replaceable>$url</replaceable>)</code></para>
          </listitem>
        </varlistentry>
        <varlistentry>
          <term>Example</term>
          <listitem>
            <programlisting>$httpclient.setProxyURL("http://user:password@proxy_host:8080/path");</programlisting>
