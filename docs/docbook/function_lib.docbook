      </listitem>
    </varlistentry>
    <varlistentry>
      <term>Example</term>
      <listitem>
	<programlisting>$secs = get_epoch_seconds(2007-09-23T00:00:01);</programlisting>
      </listitem>
    </varlistentry>
  </variablelist>

