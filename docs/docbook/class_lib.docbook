	  </listitem>
	</varlistentry>
	<varlistentry>
	  <term>Example</term>
	  <listitem>
	    <programlisting>$pool.commit();</programlisting>
	  </listitem>
	</varlistentry>
      </variablelist>

      <table>
	<title>Exceptions Thrown by DatasourcePool::commit()</title>
	<tgroup cols='2' align='left' colsep='1' rowsep='1'>
	  <thead>
	    <row>
	      <entry><para>err</para></entry>
	      <entry><para>desc</para></entry>
	    </row>
	  </thead>
