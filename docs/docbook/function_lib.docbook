        <row>
          <entry>
            <para><code><link linkend="cbrt">cbrt</link>(<link linkend="softfloat_type">softfloat</link>) <command>returns</command> <link linkend="float_type">float</link></code></para>
            <para><code><link linkend="cbrt">cbrt</link>() <command>returns</command> <link linkend="float_type">float</link> (<link linkend="RT_NOOP_flag">RT_NOOP</link>)</code></para>
