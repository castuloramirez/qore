          <term>Prototype</term>
          <listitem>
              <para><code><link linkend="int_type">int</link> <link linkend="HTTPClient_getTimeout">HTTPClient::getTimeout</link>()</code></para>
          </listitem>
        </varlistentry>
        <varlistentry>
          <term>Example</term>
          <listitem>
            <programlisting>$timeout = $httpclient.getTimeout();</programlisting>
          </listitem>
        </varlistentry>
      </variablelist>
      <table>
        <title>Return Values for HTTPClient::getTimeout()</title>
        <tgroup cols="2" align="left" colsep="1" rowsep="1">
          <thead>
            <row>
              <entry>
                <para>Return Type</para>
              </entry>
              <entry>
                <para>Description</para>
              </entry>
            </row>
          </thead>
          <tbody>
            <row>
              <entry>
                <para><code><link linkend="int_type">int</link></code></para>
