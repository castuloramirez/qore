#!/usr/bin/env qore
%new-style

%requires QUnit

%require-types
%enable-all-warnings

%requires ../../../../qlib/Util.qm

public class TmpLocationTest inherits QUnit::Test {
    constructor() {
        testFunctions = (
            ("func": \testTmpLocation(), "name": "Test presence of tmp_location()"),
        );
    }

    bool haveLocation() {
        if (tmp_location().length() > 0) {
            return True;
        }
        return False;
    }

    testTmpLocation() {
        testAssertion(\haveLocation(), ());
    }
}

return new TmpLocationTest().main();
