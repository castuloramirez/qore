#!/usr/bin/env qore
%require-types
%enable-all-warnings
%requires UnitTest
%allow-bare-refs
%exec-class App

class App inherits UnitTest {
    constructor() {
        cmp(sprintf("%04d-%.2f", 25, 101.239), "0025-101.24", "sprintf()");
        cmp(vsprintf("%04d-%.2f", (25, 101.239)), "0025-101.24", "vsprintf()");
        cmp(f_sprintf("%3s", "niña"), "niñ", "UTF-8 f_sprintf()");
    }
}


